<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap   
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"   
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">  
<sqlMap namespace="com.cy.xtgl.dao.QyJsqxwhMapper">

	<!--根据主键取DOMAIN-->
	<select id="selectXtJsGnmkByKey" parameterClass="java.util.Map"
		resultClass="com.cy.xtgl.domain.XtJsGnmkDomain">
		select a.JS_DJXH as jsDm,a.GNMK_DM as gnmkDm from QY_JS_GNMK a  where a.JS_DJXH=#jsDm# and a.GNMK_DM=#gnmkDm# and a.XTML_XH=#xtml#
    </select>

	<!--插入数据-->
	<insert id="insertXtJsGnmk"  parameterClass="com.cy.common.bo.XtJsGnmk">
		insert into QY_JS_GNMK(JS_DJXH,XTML_XH,GNMK_DM)
		values(#jsDm:VARCHAR#,#xtml:VARCHAR#,#gnmkDm:VARCHAR#)
	</insert>

	<!--根据主键更新该表中其它所有字段-->
    <update id="updateXtJsGnmkByKey" parameterClass="com.cy.common.bo.XtJsGnmk">
        update QY_JS_GNMK set JS_DJXH=#jsDm#,GNMK_DM=#gnmkDm# where JS_DJXH=#jsDm# and GNMK_DM=#gnmkDm#
    </update>
    
	<!--根据主键删除表数据：软删除-->
	<delete id="deleteXtJsGnmkByKey" parameterClass="java.util.Map">
		delete QY_JS_GNMK where JS_DJXH=#jsDm# and GNMK_DM=#gnmkDm#
    </delete>
</sqlMap>
