<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<!--  -->
<sqlMap >
 <typeAlias type="tf56.contract.domain.SettleStepSet" alias="settleStepSet"/>
 <resultMap id="settleStepSetFieldMap" class="settleStepSet">
  <result column="settleStepSetId" property="settlestepsetid" jdbcType="VARCHAR" />   <!--  -->
  <result column="settleSetId" property="settlesetid" jdbcType="VARCHAR" />   <!-- 编码Id -->
  <result column="startValue" property="startvalue" jdbcType="VARCHAR" />   <!--  -->
  <result column="endValue" property="endvalue" jdbcType="VARCHAR" />   <!--  -->
  <result column="step" property="step" jdbcType="VARCHAR" />   <!-- 阶梯 -->
  <result column="unitPrice" property="unitprice" jdbcType="VARCHAR" />   <!-- 报价 -->
 </resultMap>
 <!-- 获取多条记录查询列表 返回List -->
 <select id="iBatisSelectSettleStepSetList" parameterClass="java.util.HashMap" resultClass="settleStepSet">
  SELECT * FROM SettleStepSet WHERE settleSetId = #settlesetid#
 </select>
</sqlMap>