<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta http-equiv="pragma" content="no-cache"/>
    <meta http-equiv="Cache-Control" content="no-cache,must-revalidate"/>
    <title>选择发货方</title>
    <link href="../css/webui/hgrid.css" rel="stylesheet" />
	<script src="../js/jquery/jquery-1.8.3.min.js" type="text/javascript"></script>
	<script src="../js/themes/consignorc/consignor_import.js" type="text/javascript"></script>
	<script type="text/javascript" src="../js/sys/webui/hgrid.js"></script>
	<style type="text/css">
	 .btn{
	  display: inline-block;
  	  position: relative;
  	  margin: 10px;
  	  padding: 0 20px;
 	  text-align: center;
   	  text-decoration: none;
      font: bold 12px/25px Arial, sans-serif;
      text-shadow: 1px 1px 1px rgba(255,255,255, .22);
      -webkit-border-radius: 2px;
      -moz-border-radius: 2px;
      border-radius: 2px;
	}
	</style>
	<script>
		$(function(){
			$("#currentPartyId").val(getData());
			f_hgrid_ini();
			$("#pagecode").val("1");
			var params="partyid="+getData()+"&partytype=发货方";
 	 		f_hgrid_json(params);
 	 		$(".page_nav").next().remove();
 	 		})
	</script>
  </head>
  <body>
    <table style="width:100%;margin-bottom:0px;">
		<tr>
			<td style="text-align:left;border:0px;">
				会员名<input id="partyname" type="text" style="width:150px;margin-left:10px;height:25px"/>
				&nbsp;&nbsp;发货方&nbsp;&nbsp;<input id="consignor" type="text" style="width:150px;height:25px"/>
				<input type="button" value="查询" style="width:70px;height:25px;margin-left:10px;padding:0px;border:1px solid #999999;cursor:pointer;background-color:white" class="btn" onclick="f_SearchClick()"/>
			</td>
		</tr>
	</table>
	<table id="hgrid" cellspacing="0"></table>
	<div style="text-align:center">
		<input id="btn_sure" type="button" value="确定" class="btn" onclick="f_Sure()" style="width:90px;height:35px;padding:0px;border:0px;cursor:pointer;background-color:#ec6110;color:white" />
		<input id="btn_cancel" type="button" value="取消" class="btn" onclick="f_Cancel()" style="width:90px;height:35px;margin:0px;padding:0px;border:0px;cursor:pointer;background-color:#ec6110;color:white" />
		<input id="currentPartyId" type="hidden" value=""/>
	</div>
  </body>
</html>
