<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>调度跟踪</title>
<meta http-equiv="keywords" content="keyword1,keyword2,keyword3" />
<meta http-equiv="description" content="this is my page" />
<meta http-equiv="content-type" content="text/html; charset=UTF-8" />
<link href="../css/webui/main.css" rel="stylesheet" />	
<script src="../js/jquery/jquery-1.8.3.min.js" type=text/javascript></script>
<script src="../js/sys/system.js" type="text/javascript"></script>
<script src="../js/sys/webui/contract/header.js"></script>    	
<script src="../js/sys/webui/logo.js"></script>   	 
<script src="../js/sys/webui/contract/left.js"></script>
<link href="../js/sys/ymPrompt/skin/ch/ymPrompt.css" rel="stylesheet" />
<link href="../css/themes/waybilldistributionc/waybilldistribution.css" rel="stylesheet" />	
<link href="../css/webui/hgrid.css" rel="stylesheet" />
<script type="text/javascript" src="../js/sys/DatePicker/WdatePicker.js"></script>
<script type="text/javascript" src="../js/sys/ymPrompt/ymPrompt.js"></script>
<script type="text/javascript" src="../js/sys/webui/hgrid.js"></script>
<link type="text/css" rel="stylesheet" href="../css/webui/commonPage.css" />
<script type="text/javascript" src="../js/sys/webui/SelectSimu.js"></script>
<script type="text/javascript" src="../js/themes/dispatchtrackc/dispatchtrack.js"></script>
<style>
	.tdleft{
		padding-left: 3px;
	}
</style>
<script>
	$(function(){
		$('.status').SelectSimu({
				width : 110,
				zIndex : 1,
				listNum : 5,
				listValue : ['0','1','2','3','4'],
				listOption : ['全部','待确认','已确认','已发车','已到车'],
				imgSrc : '../imgs/sys/icon_down.png'
		});
		var systemdispatchnumber = trim($("#systemdispatchnumber").val()),
			fromdate=trim($("#fromdate").val()),
			todate=trim($("#todate").val()),
			carplatenumber=trim($("#carplatenumber").val()),
			drivermobile=trim($("#drivermobile").val()),
			status=trim($('.status div span').text()),
			paperdispatchnumber=trim($("#paperdispatchnumber").val());
		if(status=='全部'){
			status='';
		}
		var params="systemdispatchnumber="+systemdispatchnumber+"&fromdate="+fromdate+"&todate="+todate+
			"&carplatenumber="+carplatenumber+"&drivermobile="+drivermobile+"&status="+status+"&paperdispatchnumber="+
			paperdispatchnumber+"&random="+Math.random();
		f_hgrid_ini();
		$("#pagecode").val("1"); 	
		f_hgrid_json(params);
	});
</script>
</head>

<body>
	<!-- 顶部 --开始  此处须由js动态加入-->
		<div id="header" class="c-header" >
	
		</div>
		<!-- 顶部 --结束-->
	
		<!-- LOGON --开始   此处须由js动态加入-->
		<div id="logo" class="c-logo" >
		
		</div>
	<!-- LOGON --结束-->

	<div id="main" class="c-main">
		
  		<div id="left" class="c-left">
			<div id="accordion"> 
			</div>  
		</div>
  		<div id="right" class="c-right">
  				<div class="crumbs fn-mt10 fn-cGray3">
						<span class="house"></span>
						<span class="links"><a href="../logincs/index_main" class="fn-cBlue">首页</a>
						<span class="fn-ml5 fn-mr5">&gt;</span><b>调度跟踪</b></span>
				</div>
				<div class="fgxDiv"></div>
				<table class="formTable" style="width:820px;">
					<tr>
						<td class="rightTd" width="60px">调度单号</td>
						<td width="120px"><input id="systemdispatchnumber" type="text" class="com_input"/></td>
						<td class="rightTd" width="80px">调度单状态</td>
						<td width="120px">
							<div class="status"></div>
						</td>
						<td class="rightTd" width="60px">调度日期</td>
						<td width="300px">
							<input type="text" name="fromdate" id="fromdate" class="Wdate comDate" onFocus="WdatePicker({firstDayOfWeek:1})"style="width: 100px;"/>
							-
							<input type="text" name="todate" id="todate"style="width: 100px;" class="Wdate comDate" onFocus="WdatePicker({firstDayOfWeek:1})"/>
						</td>
					</tr>
					<tr>
						<td class="rightTd">车牌号</td>
						<td width="120px;">
							<input id="carplatenumber" type="text" class="com_input"/>
						</td>
						<td class="rightTd">司机手机</td>
						<td ><input id="drivermobile" type="text" class="com_input"/></td>
						<td class="rightTd" style="width:120px;;">纸质调度单号</td>
						<td>
							<input id="paperdispatchnumber" type="text" class="com_input"/>
							<button class="btn-white fn-noborder" style="margin-left: 20px;" onclick=f_search()>查询</button>
						</td>
					</tr>					
				</table>
		  		<div id="hgrid_div" style="margin-left: 0px;">
		        	<div  style="float: left;overflow-x:scroll;width:670px;min-height:60px; ">
		  					<table id="hgrid"  style="width:1120px;" cellspacing="0"></table>
		  			</div>		 
		  			<div style="width: 150px;float: left;min-height:60px;">
		  					<table id="operation"  style="width:150px;" cellspacing="0"></table>
		  			</div> 			
	  			</div>
       		</div>
  		</div>
</body>
</html>
