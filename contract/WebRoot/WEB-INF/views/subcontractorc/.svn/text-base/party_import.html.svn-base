<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <title>会员导入</title>
    <meta http-equiv="keywords" content="keyword1,keyword2,keyword3">
    <meta http-equiv="description" content="this is my page">
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    <script src="../js/jquery/jquery-1.8.3.min.js" type=text/javascript></script>
	<link href="../js/sys/ymPrompt/skin/ch/ymPrompt.css" rel="stylesheet">
	<link href="../css/webui/hgrid.css" rel="stylesheet" />
    <script type="text/javascript" src="../js/sys/DatePicker/WdatePicker.js"></script>
	<script type="text/javascript" src="../js/sys/ymPrompt/ymPrompt.js"></script>
	<script type="text/javascript" src="../js/sys/webui/hgrid.js"></script>
  <style>
#btnsave{
	background-color: #EC6110;
    border: 0 none;
    color: #FFFFFF;
    cursor: pointer;
    height: 35px;
    margin: 0;
    padding: 0;
    width: 90px;
    font-size:14px;
	font-weight:bold;
	}
#reback{
	font-size:14px;
	font-weight:bold;
	background-color: #EC6110;
    border: 0 none;
    color: #FFFFFF;
    cursor: pointer;
    height: 35px;
    margin: 0;
    padding: 0;
    width: 90px;
    margin-left: 15px;} 
#top{width: 600px;margin-left: 20px;color: #444444;}
#btnquery{width: 48px;height: 27px;line-height: 24px;cursor: pointer;}
#organization:hover,#partyname:hover{width: 160px;height: 28px;line-height: 24px;font-size: 12px;color: #444444;border:1px solid #a1a3a6;}
#organization,#partyname{width: 160px;height: 28px;line-height: 24px;font-size: 12px;color: #444444;border:1px solid #d3d7d4;}
#hgrid td{ color: #444444;}
.page_nav a{color: inherit;color: #444444;}
  </style>
  </head>
  <body style="overflow: hidden;color: #444444;">
  	<div style="width: 600px;">
  		<div id="top">
  			<label>会员名</label>
  			<input id="partyname" name="partyname"/>
  			<label style="margin-left: 30px;">分包商名称</label><input id="partyid1" type="hidden"/>
  			<input id="organization" name="organization"/>
  			<input id="btnquery" name="btnquery" type="button" value="查询"/>
  		</div>
		<div style="width: 600px;margin-top: 5px;">
  			<table id="hgrid" style="margin-left:12px;"></table>
  		</div>
  		<div id="button" style="width: 600px;margin-top: 20px;margin-left: 32%;">
  			<div >
  				<input name="btnsave" id="btnsave" type="button" value="确定"/>
  				<input name="reback" id="reback" type="button" value="取消"/>
  			</div>
  		</div>
  	</div>
  		<script type="text/javascript" src="../js/themes/subcontractorc/party_import.js"></script>
    <script>
 	 f_hgrid_ini();
 	 $(".page_nav").next().remove();
 	  $("#partyid1").val(getid().split("&")[1].split("=")[1]);//总包商会员id
 	 if(getid()==""){
 		 $("#partyname").val("请输入关键字");
 	 }else{
 	 	 var partyname = getid().split("&")[0];
 	  	 $("#partyname").val(decodeURI(partyname));
 	 }
 	 $("#pagecode").val("1");
 	 var organization=trim($("#organization").val());var partyname=trim($("#partyname").val());
 	 if(organization=='请输入关键字'){
 	 	organization="";
 	 }
 	 if(partyname=='请输入关键字'){
 	 	partyname="";
 	 }
 	 var params="organization="+organization+"&partyname="+partyname+"&partytype=分包"+"&random="+Math.random();
 	 f_hgrid_json(params);
 	 $("#btnquery").click(function(){
 	  	 var organization=trim($("#organization").val());var partyname=trim($("#partyname").val());
 	 if(organization=='请输入关键字'){
 	 	organization="";
 	 }
 	 if(partyname=='请输入关键字'){
 	 	partyname="";
 	 }
 	  	 var params1="organization="+organization+"&partyname="+partyname+"&partytype=分包"+"&random="+Math.random();
 	 	 $("#pagecode").val("1");
 	 	 f_hgrid_json(params1);
 	 });
 function getid() {
	var idUrl = document.URL;
	var num = idUrl.indexOf("=")
	idUrl = idUrl.substr(num + 1);
	return idUrl;
}	 
  </script> 	
  </body>
</html>
