<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
  <head>
    <title>结算设置详情</title>
	
    <meta http-equiv="keywords" content="keyword1,keyword2,keyword3">
    <meta http-equiv="description" content="this is my page">
    <meta http-equiv="content-type" content="text/html; charset=UTF-8">
    
	<style type="text/css">
		.btn{
			display: inline-block;
		  	position: relative;
		 	text-align: center;
		   	ext-decoration: none;
		    font: bold 12px/25px Arial, sans-serif;
		    text-shadow: 1px 1px 1px rgba(255,255,255, .22);
		    -webkit-border-radius: 2px;
		    -moz-border-radius: 2px;
		    border-radius: 2px;
		}
	</style>
	<link href="../css/webui/hgrid.css" rel="stylesheet" />
	<link href="../js/sys/ymPrompt/skin/ch/ymPrompt.css" rel="stylesheet" />
	<script src="../js/jquery/jquery-1.8.3.min.js" type=text/javascript></script> 
	<script src="../js/sys/ymPrompt/ymPrompt.js" type=text/javascript></script>
	<script src="../js/themes/settlec/settle_detail.js" type="text/javascript"></script>
  </head>
  
  <body>
  	<div style="margin:0px 10px;">
  		<div class="top_content">
			<span style="color:#1560ea;font-size:14px;margin-left:10px" id="tradeType"></span>
			<span style="color:#1560ea;font-size:14px" id="partyname"></span>
			<span style="color:#1560ea;font-size:14px;margin-left:80px">结算类型：</span>
			<span style="color:#1560ea;font-size:14px" id="type"></span>
			<span style="color:#1560ea;font-size:14px;margin-left:130px">报价方式：</span>
			<span style="color:#1560ea;font-size:14px" id="reporttype"></span>
  		</div>
  		<div class="middle_content">
  			<div style="margin-top:20px;margin-bottom:10px">
  				<span><b>结算单价明细</b></span>
  			</div>
  			<table cellspacing="0">
  				<tr height="30px">
					<th class="tdleft"  width="150px" scope="col" style="font-weight:normal">发货地</th>
					<th class="tdleft"  width="150px" scope="col" style="font-weight:normal">收货地</th>
					<th class="tdleft"  width="90px" scope="col" style="font-weight:normal">重货单价(元/kg)</th>
					<th class="tdleft"  width="90px" scope="col" style="font-weight:normal">泡货单价(元/m³)</th>
					<th class="tdleft"  width="100px" scope="col" style="font-weight:normal">吨公里单价(元/吨.公里)</th>
					<th class="tdleft"  width="120px" scope="col" style="font-weight:normal">方公里单价(元/立方.公里)</th>
					<th class="tdleft"  width="90px" scope="col" style="font-weight:normal">整车单价(元/车)</th>
  				</tr>
  			</table>
  			<div style="width:760px;height:210px;overflow-y:auto">
  				<div id="loading" style="text-align:center;padding:0px"><img src="../imgs/sys/loading.gif" border="0" /></div>
  				<table id="hgrid" cellspacing="0" style="margin:0px"></table>
  			</div>
  		</div>
  		<div style="margin:20px auto 10px;text-align:center" id="button">
  			<input id="btnClose" class="btn" type="button" onclick="window.parent.ymPrompt.doHandler('close',true);" value="关闭" style="width:80px;height:35px;background-color:#ec6110;color:white;font-size:larger;padding:0px;border:0px;cursor:pointer;">
  		</div>
  	</div>
  	<script type="text/javascript">
  		function getparams(){
			var idUrl= document.URL;
			var num=idUrl.indexOf("?") 
			idUrl=idUrl.substr(num+1);
			var params=decodeURI(idUrl);
			return params;
 		}
		$(document).ready(function(){
			var params=getparams();
			var paramsarray=params.split("&");	
			var type=paramsarray[0].split("=")[1];
			var topartyid = paramsarray[1].split("=")[1];
			var partyname = paramsarray[2].split("=")[1];
			var reporttype = paramsarray[3].split("=")[1];
			var tradeType = paramsarray[4].split("=")[1];
			var url="type="+type+"&topartyid="+topartyid+'&random='+Math.random();
			$('#partyname').html(partyname);
			$('#type').html(type);
			$('#reporttype').html(reporttype);
			$('#tradeType').html(tradeType+"：");
			f_hgrid_json(url);
		});
			
  	</script>
  </body>
</html>
