@charset "utf-8";
*{margin:0;padding:0;}
body {
min-height:820px;font-size:12px;color:#3f3f3f;font-family:"微软雅黑","宋体",Arial, Helvetica, sans-serif; margin:0 auto;
}
ul{list-style:none;}

a{color:#1560ea;text-decoration:none;}
a:hover{color:#1560ea; text-decoration:underline;}
/*header*/
.c-header{min-width:1000px;width:100%;margin:0 auto;}

/*logo*/
.c-logo{min-width:1000px;width:100%;height:85px;background-color:#f8f8f8;margin:0px auto;border:0px solid red;border-bottom:2px solid #ec6110;text-align:center;}



/*main*/
.c-main{width:998px;border:0px solid black;padding:0px;margin:0 auto;text-align:center;}

/*left*/
.c-left{width:150px;font-size:14px;background-color:#f5f5f5;margin-top:20px;margin-left:0px;border:0px solid #999999;padding:0px;float:left;}

/*right*/
.c-right{width:820px;margin-left:25px;margin-top:20px;border:0px solid #dddddd;padding:0px;float:left;}

/*footer*/
.c-footer{width:1000px;border-top:0px solid #dddddd;padding:0px;margin:20px auto;}
