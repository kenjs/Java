/*css-sprites*/
.up-arrow,.down-arrow,.house,.complete,.unfinished,.warning-yellow,.warning-blue,.success,.time-icon,.btn-white,.btn-white-hover,.btn-orange-small,.btn-orange-small-hover,.btn-orange-big,.btn-orange-big-hover{background:url(../../imgs/sys/icons.png) no-repeat 0 0; display:inline-block; vertical-align:middle;}
.up-arrow{width:11px;height:6px;background-position:-1px -1px;}
.down-arrow{width:11px;height:6px;background-position:-1px -8px;}
.house{width:17px;height:16px;background-position:-1px -16px;}
.unfinished{width:25px;height:25px;background-position:-49px -7px;}
.complete{width:25px;height:25px;background-position:-23px -7px;}
.warning-yellow{width:43px;height:38px;background-position:-26px -34px;}
.warning-blue{width:43px;height:38px;background-position:-43px -77px;}
.success{width:41px;height:41px;background-position:-2px -77px;}
.time-icon{width:15px;height:16px;background-position:-2px -33px;}
.btn-white{width:59px;height:27px;background-position:-77px -8px;}
.btn-white:hover{width:59px;height:27px;background-position:-77px -36px;}  
.btn-white-hover{width:59px;height:27px;background-position:-77px -36px;}
.btn-orange-small{width:85px;height:26px;background-position:-138px -37px;}
.btn-orange-small:hover{width:85px;height:26px;background-position:-138px -9px;}
.btn-orange-small-hover{width:85px;height:26px;background-position:-138px -37px;}
.btn-orange-big{width:85px;height:35px;background-position:-227px -8px;}
.btn-orange-big:hover{width:85px;height:35px;background-position:-227px -45px;}
.btn-orange-big-hover{width:85px;height:35px;background-position:-227px -45px;}

/*面包屑*/
.crumbs{height:25px; line-height:18px; position:relative; width:100%;text-align: left;}
.crumbs .house{position:absolute; top:0px; left:0px;}
.crumbs .links{padding-left:27px;}

/*tab切换*/
.tab-change-layer{border-bottom:solid 1px #ccc;height:34px; position:relative;border-left:solid 1px #ccc;}
.tab-change-layer .tab-change-title{ position:absolute;top:0px;left:0px;}
.tab-change-title li{ width:110px; text-align:center; height:32px; line-height:32px; border:solid 1px #ccc; border-left:none; border-bottom:none; float:left; padding-top:1px; cursor:pointer;}
.tab-change-title li.current{ border-top:solid 2px #ec6110; border-bottom:solid 1px #fff;font-weight:bold; padding-top:0px;}

/*物流进度*/
.progress li{ float:left; width:125px;}
.progress li .line{ position:relative; height:3px; width:125px; margin:20px 0px; background:#999;}
.progress li .line.fn-bg-blue{ background:#86c420;}
.progress li .line .status{ position:absolute;top:-10px;left:0px;}
.progress li .line.last{ background:none;}

